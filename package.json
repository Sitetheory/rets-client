{
  "name": "rets-client",
  "version": "5.1.0",
  "description": "A RETS client (Real Estate Transaction Standard).",
  "main": "index.js",
  "types": "dts/index.d.ts",
  "dependencies": {
    "@esvinson/formidable": "1.1.2",
